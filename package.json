{
	"name": "weather-server",
	"version": "1.0.0",
	"private": "true",
	"main": "./dist/src/index.js",
	"type": "module",
	"scripts": {
		"build": "npx tsc",
		"start": "node dist/src/index.js",
		"dev": "concurrently \"npx tsc --watch\" \"nodemon dist/src/index.js\"",
		"docker": "docker compose -f docker-compose-dev.yml up --build",
		"db:generate": "prisma generate",
		"db:push": "prisma db push",
		"db:seed": "prisma db seed",
		"db:studio": "prisma studio",
		"format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json|prisma)\"",
		"postinstall": "pnpm dlx prisma generate"
	},
	"devDependencies": {
		"concurrently": "^8.2.1",
		"nodemon": "^3.0.1",
		"prettier": "^3.0.2",
		"prettier-plugin-prisma": "^5.0.0",
		"ts-node": "^10.9.1"
	},
	"dependencies": {
		"@prisma/client": "5.10.2",
		"@types/compression": "^1.7.2",
		"@types/express": "^4.17.17",
		"@types/morgan": "^1.9.5",
		"@types/node": "^20.5.6",
		"@types/web-push": "^3.6.1",
		"bullmq": "^5.4.6",
		"compression": "^1.7.4",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-validator": "^7.0.1",
		"helmet": "^7.0.0",
		"morgan": "^1.10.0",
		"prisma": "^5.10.2",
		"redis": "^4.6.8",
		"typescript": "^5.2.2",
		"web-push": "^3.6.6"
	},
	"engines": {
		"node": "20.11.1"
	},
	"prisma": {
		"seed": "node --loader ts-node/esm prisma/seed.ts"
	}
}
